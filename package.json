{
    "name": "@truesparrow/common-server-js",
    "version": "0.2.3",
    "description": "Things shared between packages, for server-side work.",
    "author": "CHM SQRT2 Team",
    "scripts": {
        "build": "tsc",
        "test-dev": "tsc --watch & mocha --watch $(find out -name '*-test.js')",
        "test": "tsc && istanbul cover --config ./.istanbul.yml ./node_modules/mocha/bin/_mocha --  $(find out -name '*-test.js') && remap-istanbul -i ./build/coverage/coverage-final.json -o ./build/coverage/coverage-remapped.json && coverage-filter -i ./build/coverage/coverage-remapped.json -o ./build/coverage/coverage-filtered.json",
        "push-coverage-to-codecov": "codecov -f ./build/coverage/coverage-filtered.json",
        "docs": "typedoc src/"
    },
    "keywords": [
        "common",
        "server"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/truesparrow/common-server-js.git"
    },
    "homepage": "https://github.com/truesparrow/common-server-js#readme",
    "license": "MIT",
    "main": "./index.js",
    "types": "./index.d.ts",
    "filesPack": {
        "out": "e:.",
        "README.md": "f:README.md",
        "package.json": "f:package.json",
        "tsconfig.json": "f:tsconfig.json"
    },
    "dependencies": {
        "@truesparrow/common-js": "latest",
        "@types/bunyan": "=1.8.3",
        "@types/continuation-local-storage": "=3.2.1",
        "async-middleware": "=1.2.1",
        "body-parser": "=1.18.2",
        "bunyan": "=1.8.12",
        "bunyan-loggly": "=1.3.3",
        "continuation-local-storage": "=3.2.1",
        "es6-promise": "=4.1.1",
        "express": "=4.16.2",
        "express-bunyan-logger": "=1.3.2",
        "http-status-codes": "=1.3.0",
        "raynor": "=0.14.0",
        "rollbar": "=2.3.2"
    },
    "devDependencies": {
        "@truesparrow/togemfury": "latest",
        "@types/body-parser": "=1.16.8",
        "@types/chai": "=4.0.8",
        "@types/express": "=4.0.39",
        "@types/http-status-codes": "=1.0.29",
        "@types/istanbul": "=0.4.29",
        "@types/mocha": "=2.2.44",
        "@types/node": "=8.0.57",
        "chai": "=4.1.2",
        "codecov": "=3.0.0",
        "coverage-filter": "=1.0.0",
        "istanbul": "=0.4.5",
        "mocha": "=4.0.1",
        "remap-istanbul": "=0.9.5",
        "testdouble": "=3.2.6",
        "typedoc": "=0.9.0",
        "typescript": "=2.6.2"
    }
}
